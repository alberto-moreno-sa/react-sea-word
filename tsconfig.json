{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "pages/*": ["src/pages/*"],
      "configs/*": ["src/configs/*"],
      "configs": ["src/configs/index"],
      "utils/*": ["src/utils/*"],
      "utils": ["src/utils/index"],
      "store/*": ["src/store/*"],
      "store": ["src/store/index"],
      "styles/*": ["src/styles/*"],
      "images/*": ["src/images/*"],
      "components/*": ["src/components/*"],
      "services/*": ["src/services/*"],
      "services": ["src/services/index"],
      "lib/*": ["src/lib/*"],
      "testUtils": ["__tests__/testUtils"]
    },
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/configs/cmsClipMxPaths.js"
  ],
  "exclude": ["node_modules"]
}
