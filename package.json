{
  "name": "sea-word-challenge",
  "version": "1.0.0",
  "description": "A tindy Sea Word  where the user can click on the word to add or remove islands ",
  "main": "index.tsx",
  "scripts": {
    "analize": "ANALYZE=true next build",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "production:start": "export NODE_OPTIONS=--max_old_space_size=8000 && npm run build && npm run start",
    "test": "jest --coverage --no-cache --detectOpenHandles --runInBand",
    "test:s": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write .",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@next/bundle-analyzer": "^12.0.10",
    "dd-trace": "^2.1.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "husky": "^7.0.4",
    "next": "^12.0.10",
    "next-i18next": "^8.3.0",
    "next-redux-wrapper": "^6.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@swc-node/jest": "^1.4.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.12",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@typescript-eslint/typescript-estree": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.5.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-standard": "^4.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-fail-on-console": "^2.2.3",
    "jest-next-dynamic": "^1.0.1",
    "postcss": "^8.4.8",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.2",
    "redux-devtools-extension": "^2.13.9",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/alberto-moreno-sa/sea-word-challenge.git"
  },
